{
  "name": "@evgenonyskiv/robotjs",
  "version": "0.6.29",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine test/**/*.js",
    "test-keyboard": "node test/keyboard.js",
    "test:win32": "jasmine test/**/*.js",
    "install": "node-gyp-build",
    "prebuild": "prebuildify -t 20.0.0 --napi --strip",
    "prebuild-darwin-universal": "prebuildify -t 22.0.0 --napi --strip --arch x64+arm64",
    "prebuild-linux-x64": "npx prebuildify --napi --strip",
    "prebuild-linux-arm64": "prebuildify -t 22.0.0 --napi --strip",
    "prebuild-win32-x86": "prebuildify -t 22.0.0 --napi --strip",
    "prebuild-win32-x64": "prebuildify -t 22.0.0 --napi --strip",
    "prebuild-win32-arm64": "prebuildify -t 22.0.0 --napi --strip"
  },
  "files": [
    "prebuilds/",
    "build/",
    "src/",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evgenonyskiv/robotjs.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "binary": {
    "napi_versions": [
      3,
      4,
      5,
      6,
      7,
      8,
      9
    ],
    "module_name": "robotjs",
    "module_path": "prebuilds/{platform}-{arch}/",
    "package_name": "{module_name}.node"
  },
  "bugs": {
    "url": "https://github.com/evgenonyskiv/robotjs/issues"
  },
  "homepage": "https://github.com/evgenonyskiv/robotjs",
  "dependencies": {
    "node-addon-api": "^8.3.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "jasmine": "^2.99.0",
    "prebuildify": "^6.0.1",
    "run-script-os": "^1.0.3",
    "tape": "^4.8.0",
    "targetpractice": "0.0.7"
  },
  "overrides": {
    "targetpractice": {
      "electron": "^30.0.0"
    }
  }
}
